# platform :ios, '9.0'

def commonPods
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'Kingfisher', '~> 3.0'
    pod 'Firebase/Database'
end

target 'ArtNYC' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ArtNYC
  commonPods

end

